generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement()) @map("_id")
  name      String
  username  String
  email     String
  password  String
  createdAt DateTime @db.Date
}

model Products {
  id        Int    @id @default(autoincrement()) @map("_id")
  name      String
  image_url String
  price     Int
  rating    Float  @db.Real
  category  String
}
